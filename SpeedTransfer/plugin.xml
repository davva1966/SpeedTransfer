               moveable="false"
               ratio="0.75"
               relationship="bottom"
               relative="org.eclipse.ui.editorss"
               showTitle="true"
               standalone="false"
               visible="true">
         </view>
         <view
               closeable="true"
               id="com.ss.speedtransfer.queryResultView"
               minimized="false"
               moveable="true"
               ratio="0.75"
               relationship="stack"
               relative="com.ss.speedtransfer.startView"
               showTitle="true"
