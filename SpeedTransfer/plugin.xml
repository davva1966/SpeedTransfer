               style="push">
         </command>
         <separator
               name="com.ss.speedtransfer.separator5"
               visible="true">
         </separator>
         <command
               commandId="com.ss.speedtransfer.refreshNavigatorCommand"
               style="push">
         </command>
         <command
               commandId="com.ss.speedtransfer.switchWorkspaceCommand"
               style="push"
               tooltip="Restart the application using a different workspace">
         </command>
         <separator
               name="com.ss.speedtransfer.separator6"
               visible="true">
         </separator>
         <command
               commandId="org.eclipse.ui.file.properties"
               style="push">
         </command>
      </menuContribution>
