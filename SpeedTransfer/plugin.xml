               moveable="true"
               ratio="0.75"
               relationship="stack"
               relative="com.ss.speedtransfer.queryResultView"
               showTitle="true"
               standalone="false"
               visible="false">
         </view>
         <view
               closeable="true"
               id="com.ss.speedtransfer.queryExcelResultView"
