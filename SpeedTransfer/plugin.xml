   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.ss.speedtransfer.handlers.NewProjectHandler"
            commandId="com.ss.speedtransfer.newProjectCommand">
      </handler>
      <handler
        class="com.ss.speedtransfer.handlers.NewFolderHandler"
        commandId="com.ss.speedtransfer.newFolderCommand">
      </handler>
      <handler
            class="com.ss.speedtransfer.handlers.NewQueryHandler"
            commandId="com.ss.speedtransfer.newQueryCommand">
         <enabledWhen>
            <reference
                  definitionId="com.ss.speedtransfer.studioVersion">
            </reference>
         </enabledWhen>
      </handler>
      <handler
            class="com.ss.speedtransfer.handlers.NewDBConnectionHandler"
            commandId="com.ss.speedtransfer.newDBConnectionCommand">
         <enabledWhen>
            <reference
                  definitionId="com.ss.speedtransfer.studioVersion">
            </reference>
         </enabledWhen>
      </handler>
      <handler
            class="com.ss.speedtransfer.handlers.RunQueryHandler"
            commandId="com.ss.speedtransfer.runQueryCommand">
         <enabledWhen>
            <or>
               <reference
                     definitionId="com.ss.speedtransfer.executableEditor">
               </reference>
               <with
                     variable="selection">
                  <and>
                     <count
                           value="1">
                     </count>
                     <iterate
                           operator="and">
                        <or>
                           <test
                                 property="org.eclipse.core.resources.contentTypeId"
                                 value="com.ss.speedtransfer.queryContentType">
                           </test>
                           <test
                                 property="org.eclipse.core.resources.contentTypeId"
                                 value="com.ss.speedtransfer.scratchPadContentType">
                           </test>
                        </or>
                     </iterate>
                  </and>
               </with>
            </or>
         </enabledWhen>
      </handler>
      <handler
            class="com.ss.speedtransfer.handlers.RefreshNavigatorHandler"
            commandId="com.ss.speedtransfer.refreshNavigatorCommand">
      </handler>
      <handler
            class="com.ss.speedtransfer.handlers.ExportToExcelHandler"
            commandId="com.ss.speedtransfer.exportToExcelCommand">
         <enabledWhen>
            <reference
                  definitionId="com.ss.speedtransfer.executableDefinition">
            </reference>
         </enabledWhen>
      </handler>
      <handler
            class="com.ss.speedtransfer.handlers.ExportToPDFHandler"
            commandId="com.ss.speedtransfer.exportToPDFCommand">
         <enabledWhen>
            <reference
                  definitionId="com.ss.speedtransfer.executableDefinition">
            </reference>
         </enabledWhen>
      </handler>
      <handler
            class="com.ss.speedtransfer.handlers.ExportToCSVHandler"
            commandId="com.ss.speedtransfer.exportToCSVCommand">
         <enabledWhen>
            <reference
                  definitionId="com.ss.speedtransfer.executableDefinition">
            </reference>
         </enabledWhen>
      </handler>
      <handler
            class="com.ss.speedtransfer.handlers.ExportToDBHandler"
            commandId="com.ss.speedtransfer.exportToDBCommand">
         <enabledWhen>
            <and>
               <reference
                     definitionId="com.ss.speedtransfer.executableDefinition">
               </reference>
               <reference
                     definitionId="com.ss.speedtransfer.studioVersion">
               </reference>
               <not>
                  <reference
                        definitionId="com.ss.speedtransfer.selectOnlyVersion">
                  </reference>
               </not>
            </and>
         </enabledWhen>
      </handler>
      <handler
            class="com.ss.speedtransfer.handlers.LastUsedExportHandler"
            commandId="com.ss.speedtransfer.exportCommand">
         <enabledWhen>
            <reference
                  definitionId="com.ss.speedtransfer.executableDefinition">
            </reference>
         </enabledWhen>
      </handler>
      <handler
            class="com.ss.speedtransfer.handlers.ToggleLazyLoadHandler"
            commandId="com.ss.speedtransfer.toggleLazyLoadCommand">
      </handler>
      <handler
            class="com.ss.speedtransfer.handlers.ExportToWindowsExecutableHandler"
            commandId="com.ss.speedtransfer.creteExecutableCommand">
         <enabledWhen>
            <and>
               <reference
                     definitionId="com.ss.speedtransfer.queryDefinition">
               </reference>
               <reference
                     definitionId="com.ss.speedtransfer.studioVersion">
               </reference>
            </and>
         </enabledWhen>
      </handler>
      <handler
            class="com.ss.speedtransfer.handlers.AdjustColumnSizeHandler"
            commandId="com.ss.speedtransfer.adjustTableColumns">
         <activeWhen>
            <reference
                  definitionId="com.ss.speedtransfer.resultViewDefinition">
