            <actionExtension
                  pattern="com.ss.speedtransfer.navigatorActionProvider">
            </actionExtension>
         </includes>
      </viewerActionBinding>
      <dragAssistant
            class="org.eclipse.ui.navigator.resources.ResourceDragAdapterAssistant"
            viewerId="com.ss.speedtransfer.navigatorView">
      </dragAssistant>
   </extension>
      <extension
         point="org.eclipse.ui.navigator.navigatorContent">
