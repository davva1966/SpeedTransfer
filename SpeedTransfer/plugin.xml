            name="SpeedTransfer Query Content"
            visibleInUI="true">
      </commonFilter>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
       	<menuContribution
            allPopups="true"
            locationURI="popup:com.ss.speedtransfer.navigatorView">
